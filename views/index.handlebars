<div>
    <h1 class="card z-depth-3 center-align">Latest NY Times News</h1>
    <div class="row">
        <div class="col s3"></div>
        <h5 class="card z-depth-3 center-align col s6">Click "Scrape" to get new articles</h5>
    </div>
</div>

<div class"row">
    <div class="col s2"></div>
    <div class="col s8 left-align">
        {{#each article}}
        <div class="card-panel">
            <h4>{{this.title}}</h4>
            <a href="/readArticle/{{this._id}}" class="waves-effect waves-light btn" id="button">Read Articles</a>
        </div>
        {{/each}}
    </div>
</div>